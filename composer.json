{
    "name": "mendelkahan/laravel-puresms",
    "description": "Laravel package for PureSMS integration.",
    "version": "1.1.1",
    "type": "library",
    "license": "MIT",
    "autoload": {
    "psr-4": {
        "Puresms\\Laravel\\": "src/",
        "Puresms\\Laravel\\Channels\\": "src/Channels/",
        "Puresms\\Laravel\\Models\\": "src/Models/"
        
    }
},

    "authors": [
        {
            "name": "Mendel Kahan",
            "email": "mendy@famkahan.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "illuminate/support": "^10.0|^11.0|^12.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Puresms\\Laravel\\PureSmsServiceProvider"
            ],
            "aliases": {
                "PureSms": "Puresms\\Laravel\\Facades\\PureSms"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
